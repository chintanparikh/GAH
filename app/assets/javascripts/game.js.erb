$(document).ready(function(){

 $('img').mouseover(function(){
      $(this).addClass('img-hover');
   });



   $('img').mouseout(function(){
      $(this).removeClass('img-hover');
  });

  $('.gif-button').click(function() {
    var id = $(this).find("#selection_gif_id").val();

    App.game.send_gif(id)


  })



})
